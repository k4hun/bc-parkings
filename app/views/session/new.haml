%h1 Log in
.col-md-6
  = form_tag(action: 'create') do
    %table.table.table-striped
      %tr
        %td= label_tag(:email)
        %td= text_field_tag(:email, nil, class: 'form-control')
      %tr
        %td= label_tag(:password)
        %td= password_field_tag(:password, nil, class: 'form-control')
      %tr
        %th
        %td= submit_tag("Log in", class: 'btn btn-primary')
